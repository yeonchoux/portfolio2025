<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.suyeon.shopProject.clothes.ClothesMapper">

<select id="findByClothes" parameterType="int" resultType="com.suyeon.shopProject.clothes.ClothesDTO">
	SELECT *
	FROM clothes
	WHERE id = #{id}
</select>

<select id="findAll" resultType="com.suyeon.shopProject.clothes.ClothesDTO">
	SELECT id, name, price, image_path AS imagePath
 	FROM clothes
</select>





	
</mapper> 